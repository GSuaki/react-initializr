{
  "name": "{{packageName}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "{{license}}",
  "main": "main.js",
  "scripts": {
    "build": "{{manager}} run clean && webpack --mode=production",
    "start": "{{manager}} run clean && webpack --mode=production --watch",
    "clean": "del coverage dist story",
    "lint": "NODE_ENV=test eslint *.js src/**/*.js",
    "test": "NODE_ENV=test jest --no-cache --coverage --forceExit",
    "test:watch": "NODE_ENV=test jest --no-cache --coverage --watchAll --forceExit",
    "pretest": "{{manager}} run clean",
    "posttest": "{{manager}} run lint",
    "prepublishOnly": "{{manager}} test && {{manager}} run build",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "{{manager}} run build && build-storybook -c .storybook -o story"
  },
  "files": [
    "main.js",
    "card.js",
    "dist/",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "bootstrap": "4.3.1",
    "react-placeholder": "3.0.2"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-react": "7.0.0",
    "@dump247/storybook-state": "1.6.1",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-backgrounds": "5.1.9",
    "@storybook/addon-info": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addon-options": "5.1.9",
    "@storybook/addon-storyshots": "5.1.9",
    "@storybook/addon-viewport": "5.1.9",
    "@storybook/react": "5.1.9",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-minify": "0.5.0",
    "css-loader": "3.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.0",
    "jest": "24.7.1",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.12.0",
    "nodemon": "1.19.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.6",
    "webpack-node-externals": "1.7.2"
  }
}